```
//原始的冒泡排序
#include<iostream>
using namespace std;
void bubblesort(int a[],int n)
{
    for(int i=0;i<n-1;i++)
    {
        for(int j=n-2;j>=i;j--)
        {
            if(a[j]>a[j+1])
                swap(a[j],a[j+1]);
        }
    }
}
int main()
{
    int a[]={3,2,6,4,8,6,5,3,-2,-2,9,0};
    bubblesort(a,sizeof(a)/sizeof(int));
    for(int i=0;i<sizeof(a)/sizeof(int);i++)
        cout<<a[i]<<" ";
    return 0;
}

```
